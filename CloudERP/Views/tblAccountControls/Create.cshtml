@model DatabaseAccess.tblAccountControl

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal">
    <h4>tblAccountControl</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <h3 class="text-danger"> @ViewBag.Message</h3>

    <div class="form-group">
        @Html.LabelFor(model => model.AccountHeadID, "Select Account Head", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("AccountHeadID", null, "Account Head", htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.AccountHeadID, "", new { @class = "text-danger" })
        </div>
    </div>

    

    <div class="form-group">
        @Html.LabelFor(model => model.AccountControlName, "Account Control", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.AccountControlName, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.AccountControlName, "", new { @class = "text-danger" })
        </div>
    </div>




    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-success" />
            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
        </div>
    </div>
</div>
}
 